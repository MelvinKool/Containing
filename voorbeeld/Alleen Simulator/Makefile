.PHONY: clean-pyc clean-build docs clean

help:
	@echo "clean - remove all build, test, coverage and Java artifacts"
	@echo "test - run tests"
	@echo "build - compile the program"

clean:
	rm -fr build/
	rm -fr target/
	find . -name '*class' -exec rm -fr {} +
	find . -name '*~' -exec rm -f {} +

test:
	@echo "To be implemented"

build:
	@echo "To be implemented"
